package com.vh.ui.web.locators;

import org.openqa.selenium.By;

/*
 * @author Harvy Ackermans
 * @date   June 29, 2017
 * @class  PatientNavigationLocators.java
 */

public class PatientNavigationLocators
{
	public static final By MNU_PLANOFCARE = By.xpath("//a[text()='Plan of Care']");
	public static final By MNU_PLANOFCAREOVERVIEW = By.xpath("//a[contains(., 'Overview')]");
	public static final By MNU_PLANOFCAREMANAGEMENT = By.xpath("//a[contains(., 'Management')]");
	public static final By MNU_PLANOFCAREHISTORY = By.xpath("//a[contains(., 'History')]");
	public static final By MNU_PLANOFCARENOTES = By.xpath("//a[contains(., 'Notes')]");
	public static final By MNU_PLANOFCARECONSOLIDATEDSTORY = By.xpath("//a[contains(., 'Consolidated Story')]");
}